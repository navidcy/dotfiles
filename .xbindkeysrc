#"xbacklight -dec 5"
"xbacklight -dec 10"
    XF86MonBrightnessDown

#"xbacklight -inc 5"
"xbacklight -inc 10"
    XF86MonBrightnessUp

"/home/adc/bin/display-switcher.sh"
    XF86LaunchA

"/home/adc/bin/display-ext.sh"
    XF86LaunchB

"/home/adc/bin/kbd_backlight.sh down"
    XF86KbdBrightnessDown

"/home/adc/bin/kbd_backlight.sh up"
    XF86KbdBrightnessUp

"mpc prev"
    XF86AudioPrev

"mpc toggle"
    XF86AudioPlay

"mpc next"
    XF86AudioNext
    
